{% extends 'base.html.twig' %}

{% block title %}Hello ReserveringenController!{% endblock %}



{% block body %}

<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <div class="input-container">

            <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
            <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
            <script>
                $( function() {
                   $( "#datepicker" ).datepicker( "option", "dateFormat", "dd-mm-yy" );
                } );
            </script>


            <h1>Voer de gewenste periode in</h1>
            <br>
            <form style="width: 100%; margin-right: 10px" action= href="{{ path('reserveringenvrij') }}" name="form" method="POST" class="dta">
                <input required type="text" name="datepicker" placeholder=" Checkin datum" id="datepicker1" size="20"><br>
                <input required type="text" name="datepicker" placeholder=" Checkout datum" id="datepicker2" size="20">
                <br>
                <br>
                <a class="btn btn-primary" href="{{ path('reserveringenvrij') }}">Check kamersss</a>
            </form>

    </div>





</div>
{% endblock %}
{% block footer %}
{% endblock %}
